  "dockerfileLines": [
      "FROM node:12.13.0-alpine",
      "RUN mkdir -p /usr/src/app",
      "WORKDIR /usr/src/app",
      "COPY ./package.json /usr/src/app/",
     
      "RUN yarn install --no-cache",
 
      "COPY ./ /usr/src/app/",
  
      "CMD [ \"yarn\", \"start\" ]"
